# ============================================
# Finance Analytics Agent - Environment Config
# ============================================
# Copy this file to .env and fill in your API keys

# ===================
# LLM APIs (Free Tiers)
# ===================
# Groq - https://console.groq.com (Recommended - 30 req/min free)
GROQ_API_KEY=your_groq_api_key_here

# Google Gemini - https://aistudio.google.com/apikey (Optional alternative)
GOOGLE_API_KEY=your_google_api_key_here

# ===================
# Market Data APIs
# ===================
# Alpha Vantage - https://www.alphavantage.co/support/#api-key (5 calls/min free)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Finnhub - https://finnhub.io/ (60 calls/min free)
FINNHUB_API_KEY=your_finnhub_key_here

# ===================
# News APIs
# ===================
# NewsAPI - https://newsapi.org/ (100 calls/day free)
NEWSAPI_KEY=your_newsapi_key_here

# ===================
# Social Media APIs
# ===================
# Reddit - https://www.reddit.com/prefs/apps
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=FinanceAnalyticsAgent/1.0

# ===================
# Application Settings
# ===================
# Environment: development, production
ENVIRONMENT=development

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Vector DB persistence directory
VECTOR_DB_PATH=./data/vectordb

# Cache settings
CACHE_ENABLED=true
CACHE_TTL_SECONDS=300
